.title-line{
    background-color: #eeeeee;
    color: #333333;
    padding: 16rpx;
    position: sticky;
    left: 0;
    top: 0;
}

.sheet-list-item{
    display: flex;
    padding: 16rpx;
    border-bottom: solid 2rpx #cccccc;
}

.sheet-list-item-pic{
    width: 200rpx;
    height: 200rpx;

    flex-shrink: 0;
    margin-right: 16rpx;
}



.sheet-list-item-name{
    font-weight: bold;
}

.sheet-list-item-editor{
    margin-top: 16rpx;
    font-size: 26rpx;
    color: #999999;
}





.songs-list-item{
    display: flex;
    padding: 16rpx;
    border-bottom: solid 2rpx #cccccc;
}

.songs-list-item-pic{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    flex-shrink: 0;
    margin-right: 16rpx;
}



.songs-list-item-name{
    font-weight: bold;
}

.songs-list-item-editor{
    margin-top: 16rpx;
    font-size: 26rpx;
    color: #999999;
}